b = AudioFftOutput::setBandwidth( a );
QVERIFY( b <= a );
QVERIFY( b == AudioFftOutput::bandwidth() );

--------------

AudioFftOutput::setRate( a );
QVERIFY( a == AudioFftOutput::rate() );

