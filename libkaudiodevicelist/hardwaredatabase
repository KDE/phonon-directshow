# Group names can be found in $KDEHOME/share/config/phonondevicesrc, you only need to remove the
# AudioCaptureDevice_/AudioOutputDevice_/AudioIODevice_ prefix
#
# The four numbers in the group name are
# vendor id:product id:subsystem vendor id:subsystem product id
# or for USB only
# vendor id:product id
#
# The last number is the ALSA device number
#
# Every entry may have a name, icon and initialPreference entry.

# Creative SB Audigy 2 ZS
[pci:1102:0004:1102:2002:playback:alsa:4]
name=Creative SB Audigy 2 ZS p16v
initialPreference=0

[pci:1102:0004:1102:2002:playback:alsa:3]
name=Creative SB Audigy 2 ZS Multichannel Playback
initialPreference=10

[pci:1102:0004:1102:2002:playback:alsa:2]
name=Creative SB Audigy 2 ZS PT Playback
initialPreference=10

[pci:1102:0004:1102:2002:playback:alsa:0]
name=Creative SB Audigy 2 ZS Standard Playback
initialPreference=37

[pci:1102:0004:1102:2002:capture:alsa:4]
name=Creative SB Audigy 2 ZS p16v
initialPreference=0

[pci:1102:0004:1102:2002:capture:alsa:2]
name=Creative SB Audigy 2 ZS Multichannel Capture
initialPreference=10

[pci:1102:0004:1102:2002:capture:alsa:1]
name=Creative SB Audigy 2 ZS Mic Capture
initialPreference=35

[pci:1102:0004:1102:2002:capture:alsa:0]
name=Creative SB Audigy 2 ZS ADC Capture
initialPreference=36

